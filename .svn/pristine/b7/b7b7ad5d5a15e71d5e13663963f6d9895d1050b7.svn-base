<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.outbound.mapper.SendMessageLogMapper">

    <resultMap type="com.ruoyi.outbound.entity.SendMessageLog" id="SendMessageLogResult">
        <result property="id"    column="id"    />
        <result property="taskId"    column="task_id"    />
        <result property="sendTime"    column="send_time"    />
        <result property="sendContent"    column="send_content"    />
        <result property="sendMobile"    column="send_mobile"    />
        <result property="sendUserId"    column="send_user_id"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
        <result property="isDelete"    column="is_delete"    />
    </resultMap>

    <sql id="selectSendMessageLogVo">
        select id, task_id, send_time, send_content, send_mobile, send_user_id, create_time, update_time, is_delete from send_message_log
    </sql>

    <select id="selectSendMessageLogList" resultMap="SendMessageLogResult" parameterType="com.ruoyi.outbound.domain.vo.SendMessageLogVo">
        <include refid="selectSendMessageLogVo"/>
        <where>
            <if test="taskId != null "> and task_id = #{taskId}</if>
            <if test="sendContent != null  and sendMessageLog.sendContent != ''"> and send_content = #{sendContent}</if>
            <if test="sendMobile != null  and sendMessageLog.sendMobile != ''"> and send_mobile = #{sendMobile}</if>
        </where>
    </select>

    <select id="selectSendMessageLogById" parameterType="Long" resultMap="SendMessageLogResult">
        <include refid="selectSendMessageLogVo"/>
        where id = #{id}
    </select>

    <insert id="insertSendMessageLog" parameterType="com.ruoyi.outbound.entity.SendMessageLog">
        insert into send_message_log
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="taskId != null">task_id,</if>
            <if test="sendTime != null">send_time,</if>
            <if test="sendContent != null">send_content,</if>
            <if test="sendMobile != null">send_mobile,</if>
            <if test="sendUserId != null">send_user_id,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="isDelete != null">is_delete,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="taskId != null">#{taskId},</if>
            <if test="sendTime != null">#{sendTime},</if>
            <if test="sendContent != null">#{sendContent},</if>
            <if test="sendMobile != null">#{sendMobile},</if>
            <if test="sendUserId != null">#{sendUserId},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="isDelete != null">#{isDelete},</if>
         </trim>
    </insert>

    <update id="updateSendMessageLog" parameterType="com.ruoyi.outbound.entity.SendMessageLog">
        update send_message_log
        <trim prefix="SET" suffixOverrides=",">
            <if test="taskId != null">task_id = #{taskId},</if>
            <if test="sendTime != null">send_time = #{sendTime},</if>
            <if test="sendContent != null">send_content = #{sendContent},</if>
            <if test="sendMobile != null">send_mobile = #{sendMobile},</if>
            <if test="sendUserId != null">send_user_id = #{sendUserId},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="isDelete != null">is_delete = #{isDelete},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteSendMessageLogById" parameterType="Long">
        delete from send_message_log where id = #{id}
    </delete>

    <delete id="deleteSendMessageLogByIds" parameterType="String">
        delete from send_message_log where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>
